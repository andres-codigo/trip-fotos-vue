{
	"name": "trip-fotos-vue",
	"version": "0.10.1",
	"private": true,
	"description": "Extension of Udemy's Vue - The Complete Guide (incl. Router & Composition API) 'Find a coach' project.",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"lint": "eslint src --config eslint.config.mjs --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint src --fix --config eslint.config.mjs --report-unused-disable-directives --max-warnings 0 && prettier --write '**/*.{js,jsx,ts,tsx,config.js,config.mjs,css,scss,json,md}'",
		"format": "prettier --write '**/*.{js,jsx,ts,tsx,config.js,config.mjs,css,scss,json,md}'",
		"check:dependencies": "mkdir -p scripts/dependency-check && depcheck --txt > scripts/dependency-check/depcheck-output.txt",
		"cy:open:e2e": "cypress open --e2e",
		"analyse": "vite-bundle-visualizer",
		"build": "vite build",
		"preview": "vite preview"
	},
	"dependencies": {
		"@vercel/analytics": "^1.5.0",
		"browser-image-compression": "^2.0.2",
		"moment": "^2.30.1",
		"vue": "^3.5.18",
		"vue-lazyload": "^3.0.0",
		"vue-multiselect": "^3.3.1",
		"vue-router": "^4.5.1",
		"vuex": "^4.0.2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.32.0",
		"@vitejs/plugin-vue": "^6.0.1",
		"cypress": "^14.5.3",
		"depcheck": "^1.4.7",
		"dotenv": "^17.2.1",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-cypress": "^5.1.0",
		"eslint-plugin-prettier": "^5.5.3",
		"eslint-plugin-vue": "^10.3.0",
		"firebase": "^12.0.0",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"sass": "^1.89.2",
		"sass-embedded": "^1.89.2",
		"vite": "^6.3.5",
		"vite-bundle-visualizer": "^1.2.1",
		"vite-plugin-eslint2": "^5.0.4",
		"vue-eslint-parser": "^10.2.0",
		"vuefire": "^3.2.2"
	},
	"engines": {
		"node": "22.x"
	},
	"browserslist": {
		"production": [
			"defaults"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
