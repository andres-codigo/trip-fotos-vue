{
	"name": "trip-fotos-app",
	"version": "0.1.0",
	"private": true,
	"description": "Extension of Udemy's Vue - The Complete Guide (incl. Router & Composition API) 'Find a coach' project.",
	"type": "module",
	"scripts": {
		"dev": "npm run lint:js && vite",
		"build": "vite build",
		"serve": "vite preview",
		"cy:open": "cypress open",
		"lint:js": "eslint src --config eslint.config.mjs --report-unused-disable-directives --max-warnings 0",
	    "lint:fix": "eslint src --fix --config eslint.config.mjs --report-unused-disable-directives --max-warnings 0 && prettier --write 'src/**/*.{js,vue,css,scss,json,md}'",
    	"lint:watch": "chokidar 'src/**/*.{js,vue}' -c 'eslint src --config eslint.config.mjs --report-unused-disable-directives --max-warnings 0'",
		"format": "prettier --write 'src/**/*.{js,vue,css,scss,json,md}'"
	},
	"dependencies": {
		"@vercel/analytics": "^1.5.0",
		"@vercel/speed-insights": "^1.2.0",
		"browser-image-compression": "^2.0.2",
		"dotenv": "^16.4.7",
		"firebase": "^11.3.1",
		"moment": "^2.30.1",
		"vue": "^3.5.13",
		"vue-lazyload": "^3.0.0",
		"vue-multiselect": "^3.2.0",
		"vue-router": "^4.5.0",
		"vuefire": "^3.2.1",
		"vuex": "^4.0.2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.6",
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.13.0",
		"@vitejs/plugin-vue": "^5.2.1",
		"cypress": "^14.0.3",
		"eslint": "^9.20.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-cypress": "^4.1.0",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-vue": "^9.32.0",
		"globals": "^15.15.0",
		"prettier": "^3.5.1",
		"sass-embedded": "^1.85.0",
		"sass-loader": "^16.0.4",
		"vite": "^6.1.0",
		"vite-plugin-eslint2": "^5.0.3",
		"vue-eslint-parser": "^9.4.3"
	}
}
